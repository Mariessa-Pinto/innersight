{
  "expo": {
    "name": "safe-area-page-change-expo",
    "slug": "safe-area-page-change-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./atom/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./atom/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.innersight.bcit",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./atom/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.innersight.bcit"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics"
    ],
    "web": {
      "favicon": "./atom/assets/favicon.png"
    }
  }
}
